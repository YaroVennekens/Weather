#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yennekens/Documents/Coding/React/React-Portfolio-Revamp/portfolio/node_modules/.pnpm/@mapbox+mapbox-gl-style-spec@13.28.0/node_modules/@mapbox/mapbox-gl-style-spec/bin/node_modules:/Users/yennekens/Documents/Coding/React/React-Portfolio-Revamp/portfolio/node_modules/.pnpm/@mapbox+mapbox-gl-style-spec@13.28.0/node_modules/@mapbox/mapbox-gl-style-spec/node_modules:/Users/yennekens/Documents/Coding/React/React-Portfolio-Revamp/portfolio/node_modules/.pnpm/@mapbox+mapbox-gl-style-spec@13.28.0/node_modules/@mapbox/node_modules:/Users/yennekens/Documents/Coding/React/React-Portfolio-Revamp/portfolio/node_modules/.pnpm/@mapbox+mapbox-gl-style-spec@13.28.0/node_modules:/Users/yennekens/Documents/Coding/React/React-Portfolio-Revamp/portfolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yennekens/Documents/Coding/React/React-Portfolio-Revamp/portfolio/node_modules/.pnpm/@mapbox+mapbox-gl-style-spec@13.28.0/node_modules/@mapbox/mapbox-gl-style-spec/bin/node_modules:/Users/yennekens/Documents/Coding/React/React-Portfolio-Revamp/portfolio/node_modules/.pnpm/@mapbox+mapbox-gl-style-spec@13.28.0/node_modules/@mapbox/mapbox-gl-style-spec/node_modules:/Users/yennekens/Documents/Coding/React/React-Portfolio-Revamp/portfolio/node_modules/.pnpm/@mapbox+mapbox-gl-style-spec@13.28.0/node_modules/@mapbox/node_modules:/Users/yennekens/Documents/Coding/React/React-Portfolio-Revamp/portfolio/node_modules/.pnpm/@mapbox+mapbox-gl-style-spec@13.28.0/node_modules:/Users/yennekens/Documents/Coding/React/React-Portfolio-Revamp/portfolio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/gl-style-format.js" "$@"
else
  exec node  "$basedir/../../bin/gl-style-format.js" "$@"
fi
